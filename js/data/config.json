				"lists": {
					"search-by-name": "/get-moderation-list/[NAME]"
				},
				"pds": {
					"users-count": "/lists/dids-per-pds"
				},
				"others": {
					"logo": "/images/logo"
				}
			}
		},
		"geo": {
			"bdc": {
				"rootUrl": "https://api-bdc.net",
				"reverseGeocodeClient": "/data/reverse-geocode-client"
			},
			"myIPio": {
				"rootUrl": "https://api.my-ip.io",
				"ip.json": "/v2/ip.json"
			}
		}
	}
